#include<stdio.h>
int main(void)
{int n,i,k,t,h,times;
scanf("%d %d",&n,&k);int number[n];
for(i=0;i<n;i++){
scanf("%d",&h);   number[i]=h;
}for(times=0;times<k;times++){

for(i=0;i<n-1;i++){
    if(number[i]>number[i+1]){
        t=number[i];number[i]=number[i+1];number[i+1]=t;
    }}
}for(i=0;i<n;i++){
    printf("%d",number[i]);if(i<n-1){printf(" ");
	}
	
}

return 0;
}
